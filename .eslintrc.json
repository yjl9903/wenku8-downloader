{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ], // eslint-config-prettier
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ], //  eslint-plugin-prettier
  "rules": {
    "@typescript-eslint/no-non-null-assertion": "off"
  },
  "prettier/prettier": [
    "error",
    {},
    {
      "usePrettierrc": false
    }
  ]
}